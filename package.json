{
  "version": "1.5.78",
  "name": "@liaisongroup/assist-api-js-client",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/infinityhealth/assist-api-js-client.git"
  },
  "scripts": {
    "dev": "bun ./scripts/dev",
    "generate": "bun ./scripts/generate",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "bun generate && svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./zod": {
      "types": "./dist/openapi.zod.d.ts",
      "svelte": "./dist/openapi.zod.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "openapi-client-axios": "^7",
    "zod": "^3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.22.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.20.1",
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/bun": "^1.2.5",
    "@types/js-yaml": "^4.0.9",
    "axios": "^1.8.3",
    "bun-plugin-yml": "^0.3.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-svelte": "^3.3.2",
    "globals": "^16.0.0",
    "highlight.js": "^11.11.1",
    "js-yaml": "^4.1.0",
    "openapi-zod-client": "^1.18.3",
    "openapicmd": "^2.6.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.9",
    "svelte": "^5.23.2",
    "svelte-check": "^4.1.5",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2",
    "zod": "^3.24.2"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
